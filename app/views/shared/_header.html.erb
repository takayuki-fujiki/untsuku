  <%= link_to image_tag("untk-logo.png", class:"untk-icon"), "/" %>
  <% if user_signed_in? %>
    <%= link_to current_user.nickname, "#", class: "username_header " %>
    <%= link_to "LogOut", destroy_user_session_path, method: :delete, class: "login_out" %>
    <%= link_to "Post", new_post_path, class: "post_registration" %>
  <% else %>
    <%= link_to "Login", new_user_session_path, class: "login_out" %>
    <%= link_to "SignIn", new_user_registration_path, class: "post_registration" %>
  <% end %>